{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@stores/*": ["./src/stores/*"],
      "@composable/*": ["./src/composable/*"],
      "@assets/*": ["./src/assets/*"],
      "@css/*": ["./src/css/*"]
    },
  },
  "include": ["*.js","*.d.ts", "src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue","*.vue"],
}
